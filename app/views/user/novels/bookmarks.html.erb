<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h4 class="headline mt-5 ml-5">ブックマークした作品</h4>
        <div class="row mt-3">
        <% @bookmarks.each do |post| %>
          <%= render 'user/shared/post_item', post: post %>
        <% end %>
      </div>
      <div class="d-flex justify-content-center">
        <%= paginate @bookmarks, theme: 'bootstrap-5' %>
      </div>
    </div>
  </div>
</div>